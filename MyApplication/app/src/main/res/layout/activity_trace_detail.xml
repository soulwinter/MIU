<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".TraceDetail">

    <com.example.myapplication.mapDrawing.TraceView
        android:id="@+id/trace_map2"
        android:layout_width="fill_parent"
        android:layout_height="350dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/traceNumber2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="20dp"
        android:text="TextView"
        android:textSize="50dp"
        app:layout_constraintBottom_toBottomOf="@+id/trace_map2"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/tracePointText2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="10dp"
        android:text="轨迹点数"
        android:textSize="20dp"
        app:layout_constraintBottom_toTopOf="@+id/traceNumber2"
        app:layout_constraintEnd_toEndOf="parent" />

    <TextView
        android:id="@+id/traceNameDetail"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        android:textSize="50dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/trace_map2" />

    <TextView
        android:id="@+id/traceDeseriptionDetail"
        android:layout_width="374dp"
        android:layout_height="153dp"
        android:layout_marginStart="19dp"
        android:layout_marginTop="10dp"
        android:text="TextView"
        android:textSize="20dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/traceNameDetail" />
</androidx.constraintlayout.widget.ConstraintLayout>